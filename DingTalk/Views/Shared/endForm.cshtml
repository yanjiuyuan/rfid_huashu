
@{
    Layout = null;
}

<el-form-item label="标题">
    <el-input v-model="ruleForm.Title" style="width:600px;" :disabled="true"></el-input>
</el-form-item>
<el-form-item label="申请人">
    <el-input v-model="ruleForm.ApplyMan" style="width:200px;"  :disabled="true"></el-input>
</el-form-item>
<el-form-item label="申请部门">
    <el-input v-model="ruleForm.Dept" style="width:200px;"  :disabled="true"></el-input>
</el-form-item>
<el-form-item label="项目名称" v-if="ruleForm.ProjectName">
    <el-input v-model="ruleForm.ProjectId + ' - ' + ruleForm.ProjectName" :disabled="true"></el-input>
</el-form-item>
